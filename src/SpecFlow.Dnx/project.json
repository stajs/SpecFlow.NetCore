{
  "name": "dotnet-specflow",
  "version": "1.0.0-beta3",
  "description": "Generate tests from SpecFlow feature files inside ASP.NET Core projects (.xproj's).",
  "authors": [ "stajs" ],

  "packOptions": {
    "tags": [ "SpecFlow", "ASP.NET Core", "ASP.NET 5", "Visual Studio 2015", "xproj" ],
    "projectUrl": "https://github.com/stajs/SpecFlow.Dnx",
    "licenseUrl": "https://github.com/stajs/SpecFlow.Dnx/blob/master/LICENSE"
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-specflow"
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "System.Xml.XPath.XDocument": "4.0.1"
  },

  "frameworks": {
    "netcoreapp1.0": {}
  },

  "runtimes": {
    "win7-x64": {},
    "win7-x86": {}
  }
}